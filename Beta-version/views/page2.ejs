<%# CATALOG %>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link  href="./css/styleCatalog.css" rel="stylesheet">
    <title>Catalog</title>
</head>
<body>
    <header class="header">
        <h1 >House Catalog</h1>
        <p>Here are the available houses in your area</p>
    </header>



    
    <section class="catalog">
        <div class="house-item">
            <div class="house-image">
                <img src="/pics/casa5.jpeg" alt="" class="image5">
            </div>
            <h2 class="house-name">Modern Villa</h2>
            <p class="house-description">Luxurious and spacious.</p>
            <p class="house-price">$335,000</p>
            <p class="house-details">4 beds | 3 baths | 2,500 sqft</p>
            <a href="/house">View Details</a>
        </div>

        <div class="house-item">
            <div class="house-image">
                <img src="/pics/casa4.jpeg" alt="" class="image4">
            </div>
            <h2 class="house-name">Cozy Cottage</h2>
            <p class="house-description">A quiet place for families.</p>
            <p class="house-price">$370,000</p>
            <p class="house-details">3 beds | 2 baths | 1,800 sqft</p>
            <a href="/house">View Details</a>
        </div>

        <div class="house-item">
            <div class="house-image">
                <img src="/pics/casa3.jpeg" alt="" class="image3">
            </div>
            <h2 class="house-name">Urban Apartment</h2>
            <p class="house-description">Centrally located apartment.</p>
            <p class="house-price">$450,000</p>
            <p class="house-details">2 beds | 1 bath | 1,200 sqft</p>
            <a href="/house">View Details</a>
        </div>

        <div class="house-item">
            <div class="house-image">
                <img src="/pics/casa1.jpeg" alt="" class="image1">
            </div>
            <h2 class="house-name">Beachfront Property</h2>
            <p class="house-description">Oceanfront view.</p>
            <p class="house-price">$950,000</p>
            <p class="house-details">5 beds | 4 baths | 3,500 sqft</p>
            <a href="/house">View Details</a>
        </div>

        <div class="house-item">
            <div class="house-image">
                <img src="/pics/casa2.jpeg" alt="" class="image2">
            </div>
            <h2 class="house-name">Mountain Retreat</h2>
            <p class="house-description">Peaceful and secluded.</p>
            <p class="house-price">$525,000</p>
            <p class="house-details">3 beds | 2 baths | 2,000 sqft</p>
            <a href="/house">View Details</a>
        </div>
    </section>

    <footer class="footer">
        <div class="footer-content">
            <p>&copy; 2024 House Search</p>
            <img src="/pics/Black White Simple House Logo.svg" alt="Logo" style="width: 150px;">
        </div>
    </footer>
    
</body>
</html>



